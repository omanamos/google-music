{
        "name": "Google Music Playlist Management",
        "version": "1.0",
        "description": "Playlist management tool for Google Music",
        "browser_action": {
             "default_icon": "images/icon48.png"
        },
        "content_scripts": [
            {
                "matches" : ["*://music.google.com/*"],
                "js" : ["js/jquery.js"],
                "run_at" : "document_start"
            }
        ],
        "permissions": [
            "*://music.google.com/*"
        ]
}
